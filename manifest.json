{
   "background": {
      "persistent": true,
      "scripts": [  "background.js" ]
   },
   "browser_action": {
      "default_icon": {
         "128": "tap.png"
      },
      "default_title": "Chrome-It"
   },
   "content_security_policy": "script-src 'self' https://www.google-analytics.com; object-src 'self'",
   "description": "Now use your voice to control your browser.",
   "icons": {
      "128": "chrom.png",
      "16": "chrom.png",
      "48": "chrom.png"
   },
   "manifest_version": 2,
   "name": "Chrome-It",
   "permissions": [ "bookmarks", "history", "geolocation", "topSites", "tabs", "storage" ],
   
   
   "version": "2.1.7"
}
